<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-pak">
        <comment>Pack file</comment>
        <magic priority="50">
            <match value="PAK" type="string" offset="0"/>
        </magic>
    </mime-type>
    <mime-type type="application/java-multimedia-file">
        <comment>Java multimedia file</comment>
        <glob pattern="*.jmf"/>
    </mime-type>
    <mime-type type="text/x-gnumakefile">
        <comment>Makefile</comment>
        <sub-class-of type="text/plain"/>
        <glob pattern="GNUmakefile"/>
    </mime-type>
    <mime-type type="application/x-otherpak">
        <comment>Some package file</comment>
        <magic priority="50">
            <match value="PACK" type="string" offset="0"/>
        </magic>
    </mime-type>
    <mime-type type="image/x-somesprite">
        <comment>Some sprite</comment>
        <magic priority="50">
            <match value="IDS2" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
