<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-jmf">
        <comment>Jackhammer map file</comment>
        <icon name="application-x-jmf"/>
        <magic-deleteall/>
        <magic priority="60">
            <match value="JHMF" type="string" offset="0"/>
        </magic>
        <glob-deleteall/>
        <glob pattern="*.jmf"/>
    </mime-type>
    
    <mime-type type="application/x-jmx">
        <comment>Jackhammer map file (backup)</comment>
        <sub-class-of type="application/x-jmf"/>
        <icon name="application-x-jmf"/>
        <magic-deleteall/>
        <magic priority="60">
            <match value="JHMF" type="string" offset="0"/>
        </magic>
        <glob-deleteall/>
        <glob pattern="*.jmx"/>
    </mime-type>
</mime-info>
